<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/assignment/type/github/db" VERSION="20111215" COMMENT="XMLDB file for Moodle mod/assignment/type/github"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="assignment_github_repos" COMMENT="Github project records" NEXT="assignment">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="course"/>
        <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="Course ID" PREVIOUS="id" NEXT="assignment"/>
        <FIELD NAME="assignment" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="Assignment ID" PREVIOUS="course" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="User ID" PREVIOUS="assignment" NEXT="groupid"/>
        <FIELD NAME="groupid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="Group ID" PREVIOUS="userid" NEXT="username"/>
        <FIELD NAME="username" TYPE="char" LENGTH="64" NOTNULL="true" SEQUENCE="false" COMMENT="Github user's name or orgnization's name" PREVIOUS="groupid" NEXT="repo"/>
        <FIELD NAME="repo" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="Repository name" PREVIOUS="username" NEXT="members"/>
        <FIELD NAME="members" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" COMMENT="Member list" PREVIOUS="repo" NEXT="url"/>
        <FIELD NAME="url" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" COMMENT="Repository's URL" PREVIOUS="members" NEXT="owner"/>
        <FIELD NAME="owner" TYPE="char" LENGTH="64" NOTNULL="true" SEQUENCE="false" COMMENT="Repository's owner" PREVIOUS="url" NEXT="repo_created"/>
        <FIELD NAME="repo_created" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="Repository's created time" PREVIOUS="owner" NEXT="created_user"/>
        <FIELD NAME="created_user" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="The user who create a record" PREVIOUS="repo_created" NEXT="updated_user"/>
        <FIELD NAME="updated_user" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="The user who update this record" PREVIOUS="created_user" NEXT="created"/>
        <FIELD NAME="created" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="Record created time" PREVIOUS="updated_user" NEXT="updated"/>
        <FIELD NAME="updated" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="Record updated time" PREVIOUS="created"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="github_repo_course" UNIQUE="false" FIELDS="course" NEXT="github_repo_assignment"/>
        <INDEX NAME="github_repo_assignment" UNIQUE="false" FIELDS="assignment" PREVIOUS="github_repo_course" NEXT="github_repo_userid"/>
        <INDEX NAME="github_repo_userid" UNIQUE="false" FIELDS="userid" PREVIOUS="github_repo_assignment" NEXT="github_repo_groupid"/>
        <INDEX NAME="github_repo_groupid" UNIQUE="false" FIELDS="groupid" PREVIOUS="github_repo_userid"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="assignment" COMMENT="assignment table retrofitted from MySQL" PREVIOUS="assignment_github_repos">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="course"/>
        <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="id" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT="" SEQUENCE="false" PREVIOUS="course" NEXT="intro"/>
        <FIELD NAME="intro" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" PREVIOUS="name" NEXT="introformat"/>
        <FIELD NAME="introformat" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="intro" NEXT="assignmenttype"/>
        <FIELD NAME="assignmenttype" TYPE="char" LENGTH="50" NOTNULL="true" DEFAULT="" SEQUENCE="false" PREVIOUS="introformat" NEXT="resubmit"/>
        <FIELD NAME="resubmit" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="assignmenttype" NEXT="preventlate"/>
        <FIELD NAME="preventlate" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="1" SEQUENCE="false" PREVIOUS="resubmit" NEXT="emailteachers"/>
        <FIELD NAME="emailteachers" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="preventlate" NEXT="var1"/>
        <FIELD NAME="var1" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="emailteachers" NEXT="var2"/>
        <FIELD NAME="var2" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="var1" NEXT="var3"/>
        <FIELD NAME="var3" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="var2" NEXT="var4"/>
        <FIELD NAME="var4" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="var3" NEXT="var5"/>
        <FIELD NAME="var5" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="var4" NEXT="maxbytes"/>
        <FIELD NAME="maxbytes" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="100000" SEQUENCE="false" PREVIOUS="var5" NEXT="timedue"/>
        <FIELD NAME="timedue" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="maxbytes" NEXT="timeavailable"/>
        <FIELD NAME="timeavailable" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="timedue" NEXT="grade"/>
        <FIELD NAME="grade" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="timeavailable" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="grade"/>
      </FIELDS>
      <INDEXES>
        <INDEX NAME="mdl_assi_cou_ix" UNIQUE="false" FIELDS="course"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>